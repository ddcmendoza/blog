<h2> WELLO HORLD!</h2>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Content</th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |article| %>
            <tr>
                <td><%= article.id %></td>
                <td><%= article.title %></td>
                <td><%= article.content %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<%= link_to 'New Article', new_article_path %>